{
    "name": "portfolio",
    "version": "1.0.0",
    "description": "A simple portfolio",
    "scripts": {
        "dev-css": "tailwind build -i src/css/app.css -o assets/css/app.css",
        "prod-css": "tailwind build -i src/css/app.css -o assets/css/app.css --minify",
        "dev-js": "webpack --mode=development --config=webpack.config.js",
        "prod-js": "webpack --mode=production --config=webpack.config.js",
        "watch": "npx tailwindcss -i src/css/app.css -o assets/css/app.css --watch"
    },
    "keywords": [
        "portfolio"
    ],
    "author": "Shafiur Shamim",
    "license": "MIT",
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.6",
        "daisyui": "^3.9.3",
        "jquery": "^3.7.1",
        "tailwindcss": "^3.3.3",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    }
}
